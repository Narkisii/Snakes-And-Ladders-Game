            <Text fx:id="gameTitle" fill="#7eff5e" layoutX="43.0" layoutY="250.0" stroke="BLACK" strokeType="OUTSIDE" text="Snakes and ladders" textAlignment="CENTER">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="95.0" />
               </font>
            </Text>
            <VBox alignment="CENTER" layoutX="288.0" layoutY="327.0" styleClass="vbox-menu">
               <children>
                  <Button id="1" fx:id="button1" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="37.0" prefWidth="326.0" style="-fx-background-color: #FDE767;" text="Start">
                     <font>
                        <Font name="Berlin Sans FB" size="36.0" />
                     </font>
                  </Button>
                  <Button id="1" fx:id="button11" mnemonicParsing="false" prefHeight="68.0" prefWidth="326.0" style="-fx-background-color: #AAD9BB;" styleClass="menu-button" text="Question Wizard">
                     <font>
                        <Font name="Berlin Sans FB" size="36.0" />
                     </font>
                  </Button>
                  <Button id="1" fx:id="button111" mnemonicParsing="false" prefHeight="37.0" prefWidth="326.0" style="-fx-background-color: AAD9BB;" text="History">
