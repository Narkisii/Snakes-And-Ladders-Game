<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="pop_anchorPane" type="AnchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.QuestionSavedControl">
   <children>
      <VBox fx:id="pop_vBox" alignment="CENTER" prefHeight="150.0" prefWidth="450.0" spacing="10.0" styleClass="qSaved_Bodybg" stylesheets="@resources/Css/popUpStyle.css">
         <children>
            <Pane minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="450.0" />
            <Label fx:id="message_Lbl" alignment="CENTER" prefHeight="42.0" prefWidth="462.0" styleClass="pop_font" stylesheets="@resources/Css/popUpStyle.css" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <ImageView fitHeight="138.0" fitWidth="133.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/smiley-thumb-up.png" />
               </image>
            </ImageView>
            <Button fx:id="okButton" prefHeight="26.0" prefWidth="62.0" styleClass="saveButton" text="OK" />
            <Pane minHeight="-Infinity" prefHeight="15.0" prefWidth="450.0" />
         </children>
      </VBox>
   </children>
</fx:root>
